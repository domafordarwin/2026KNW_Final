<section class="report-section">
  <h2>C-03 Domain Distribution</h2>
  <table class="report-table">
    <thead>
      <tr>
        <th>Domain</th>
        <th>Average</th>
        <th>Std</th>
        <th>Chart</th>
      </tr>
    </thead>
    <tbody>
      <% @presenter.domain_aggs.each do |agg| %>
        <tr>
          <td><%= agg.domain %></td>
          <td><%= agg.avg %></td>
          <td><%= agg.std %></td>
          <td><%= report_bar(agg.avg) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>
